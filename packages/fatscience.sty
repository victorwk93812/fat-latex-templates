\newtheorem{innercustomgeneric}{\customgenericname}
\ProvideDocumentCommand{\customgenericname}{}{}
\NewDocumentCommand{\newcustomtheorem}{m m}{%
\NewDocumentEnvironment{#1}{m O{}}
	{%
		\RenewDocumentCommand{\customgenericname}{}{#2}%
		\RenewDocumentCommand{\theinnercustomgeneric}{##1}%
		\innercustomgeneric[##2]
	}
	{\endinnercustomgeneric}
}
